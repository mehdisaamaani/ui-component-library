#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Project/ui-component-library/node_modules/.pnpm/chromatic@13.3.4/node_modules/chromatic/dist/node_modules:/mnt/e/Project/ui-component-library/node_modules/.pnpm/chromatic@13.3.4/node_modules/chromatic/node_modules:/mnt/e/Project/ui-component-library/node_modules/.pnpm/chromatic@13.3.4/node_modules:/mnt/e/Project/ui-component-library/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Project/ui-component-library/node_modules/.pnpm/chromatic@13.3.4/node_modules/chromatic/dist/node_modules:/mnt/e/Project/ui-component-library/node_modules/.pnpm/chromatic@13.3.4/node_modules/chromatic/node_modules:/mnt/e/Project/ui-component-library/node_modules/.pnpm/chromatic@13.3.4/node_modules:/mnt/e/Project/ui-component-library/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../chromatic@13.3.4/node_modules/chromatic/dist/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../chromatic@13.3.4/node_modules/chromatic/dist/bin.js" "$@"
fi
