#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/d/project/ui-component-library/node_modules/.pnpm/vitest@4.0.16_@types+node@2_6600819cc71c20c42f0e25a8a813aeb8/node_modules/vitest/node_modules:/proc/cygdrive/d/project/ui-component-library/node_modules/.pnpm/vitest@4.0.16_@types+node@2_6600819cc71c20c42f0e25a8a813aeb8/node_modules:/proc/cygdrive/d/project/ui-component-library/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/d/project/ui-component-library/node_modules/.pnpm/vitest@4.0.16_@types+node@2_6600819cc71c20c42f0e25a8a813aeb8/node_modules/vitest/node_modules:/proc/cygdrive/d/project/ui-component-library/node_modules/.pnpm/vitest@4.0.16_@types+node@2_6600819cc71c20c42f0e25a8a813aeb8/node_modules:/proc/cygdrive/d/project/ui-component-library/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vitest@4.0.16_@types+node@2_6600819cc71c20c42f0e25a8a813aeb8/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/vitest@4.0.16_@types+node@2_6600819cc71c20c42f0e25a8a813aeb8/node_modules/vitest/vitest.mjs" "$@"
fi
