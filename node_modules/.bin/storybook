#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Project/ui-component-library/node_modules/.pnpm/storybook@10.1.10_@testing-library+dom@10.4.1_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/storybook/dist/bin/node_modules:/mnt/e/Project/ui-component-library/node_modules/.pnpm/storybook@10.1.10_@testing-library+dom@10.4.1_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/storybook/dist/node_modules:/mnt/e/Project/ui-component-library/node_modules/.pnpm/storybook@10.1.10_@testing-library+dom@10.4.1_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/storybook/node_modules:/mnt/e/Project/ui-component-library/node_modules/.pnpm/storybook@10.1.10_@testing-library+dom@10.4.1_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules:/mnt/e/Project/ui-component-library/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Project/ui-component-library/node_modules/.pnpm/storybook@10.1.10_@testing-library+dom@10.4.1_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/storybook/dist/bin/node_modules:/mnt/e/Project/ui-component-library/node_modules/.pnpm/storybook@10.1.10_@testing-library+dom@10.4.1_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/storybook/dist/node_modules:/mnt/e/Project/ui-component-library/node_modules/.pnpm/storybook@10.1.10_@testing-library+dom@10.4.1_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/storybook/node_modules:/mnt/e/Project/ui-component-library/node_modules/.pnpm/storybook@10.1.10_@testing-library+dom@10.4.1_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules:/mnt/e/Project/ui-component-library/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../storybook/dist/bin/dispatcher.js" "$@"
else
  exec node  "$basedir/../storybook/dist/bin/dispatcher.js" "$@"
fi
