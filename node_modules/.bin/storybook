#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/d/project/ui-component-library/node_modules/.pnpm/storybook@10.1.10_@testing-_978f5bbf642511816176414421dbc724/node_modules/storybook/dist/bin/node_modules:/proc/cygdrive/d/project/ui-component-library/node_modules/.pnpm/storybook@10.1.10_@testing-_978f5bbf642511816176414421dbc724/node_modules/storybook/dist/node_modules:/proc/cygdrive/d/project/ui-component-library/node_modules/.pnpm/storybook@10.1.10_@testing-_978f5bbf642511816176414421dbc724/node_modules/storybook/node_modules:/proc/cygdrive/d/project/ui-component-library/node_modules/.pnpm/storybook@10.1.10_@testing-_978f5bbf642511816176414421dbc724/node_modules:/proc/cygdrive/d/project/ui-component-library/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/d/project/ui-component-library/node_modules/.pnpm/storybook@10.1.10_@testing-_978f5bbf642511816176414421dbc724/node_modules/storybook/dist/bin/node_modules:/proc/cygdrive/d/project/ui-component-library/node_modules/.pnpm/storybook@10.1.10_@testing-_978f5bbf642511816176414421dbc724/node_modules/storybook/dist/node_modules:/proc/cygdrive/d/project/ui-component-library/node_modules/.pnpm/storybook@10.1.10_@testing-_978f5bbf642511816176414421dbc724/node_modules/storybook/node_modules:/proc/cygdrive/d/project/ui-component-library/node_modules/.pnpm/storybook@10.1.10_@testing-_978f5bbf642511816176414421dbc724/node_modules:/proc/cygdrive/d/project/ui-component-library/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/storybook@10.1.10_@testing-_978f5bbf642511816176414421dbc724/node_modules/storybook/dist/bin/dispatcher.js" "$@"
else
  exec node  "$basedir/../.pnpm/storybook@10.1.10_@testing-_978f5bbf642511816176414421dbc724/node_modules/storybook/dist/bin/dispatcher.js" "$@"
fi
